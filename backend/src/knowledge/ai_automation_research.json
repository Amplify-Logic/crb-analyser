  "roi_calculation_methodology": {
    "formula": {
      "annual_savings": "hours_on_task × automation_rate × hourly_rate × employees × 52",
      "implementation_cost": "varies by approach (DIY/SaaS/Custom)",
      "roi_percent": "((annual_savings - annual_cost) / implementation_cost) × 100",
      "payback_months": "implementation_cost / monthly_savings"
    },
    "conservative_assumptions": {
      "adoption_rate": 0.7,
      "note": "Not all employees adopt immediately, assume 70% utilization"
    },
    "transparency_requirement": "All estimates must show assumptions and source references"
  }
